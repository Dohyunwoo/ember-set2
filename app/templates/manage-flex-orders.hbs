<div class="uk-container">
    <div uk-grid>
        <div class="uk-width-2-3@m">
            <nav class="uk-navbar uk-navbar-container uk-margin" uk-navbar>
                <div class="uk-navbar-left">
                    <div class="uk-navbar-item">
                        <div class="uk-offcanvas-content">
                            <a class="uk-navbar-toggle" href="#" uk-toggle="target: #offcanvas-nav-primary">
                                <span uk-navbar-toggle-icon></span> 
                                <span class="uk-margin-small-left">Menu</span>
                            </a>
                        </div>
                    </div>
                    <ul class="uk-navbar-nav">
                        <li>
                            <a href="#">신규 주문<span class="uk-badge">({{model.length}})</span></a>
                        </li>
                        <li>
                            <a href="#">주문 처리중<span class="uk-badge">({{model.[0].local}})</span></a>
                            <div uk-dropdown>
                                <ul class="uk-nav uk-dropdown-nav">
                                    <li><a href="#">주문 수락 대기</a></li>
                                    <li><a href="#">주문 완료</a></li>
                                    <li><a href="#">번들 대기</a></li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <a href="#">주문 처리 완료<span class="uk-badge">({{model.[2].local}})</span></a>
                            <div class="uk-navbar-dropdown">
                                <ul class="uk-nav uk-navbar-dropdown-nav">
                                    <li><a href="#">주문 취소 완료</a></li>
                                    <li><a href="#">주문 수취 완료</a></li>
                                </ul>
                            </div>
                        </li>
                    </ul>
                    <div class="uk-navbar-item">
                        <form class="uk-search uk-search-default">
                            <span uk-search-icon></span>
                            <input class="uk-search-input uk-width-1-1" type="search" placeholder="Search...">
                        </form>
                    </div> 
                </div>
            </nav>
        </div>
    </div>
    <div class="uk-overflow-auto uk-table-middle uk-height-large">
        <table class="uk-table uk-table-striped uk-table-hover uk-table-small uk-table-condensed">
            <thead>
            <tr>
                <th>State</th>
                <th>Reception Date</th>
                <th>Confirm Date</th>
                <th>Order Number</th>
                <th>ASIN(Product ID)</th>
                <th class="uk-width-small">Local Tracking Number(Link)</th>
                <th class="uk-width-small">International Tracking Number</th>
                <th>Customer Response</th>
                <th>Detail View</th>
            </tr>
            </thead>
            <tbody class="uk-text-small">
                {{#each model as |rental|}}
                <tr>
                    <td>{{rental.state}}</td>
                    <td>{{rental.receptionDate}}</td>
                    <td>{{rental.confirmDate}}</td>
                    <td>{{rental.orderNumber}}</td>
                    <td>{{rental.asin}}</td>
                    <td>{{rental.localTrackingNumber}}</td>
                    <td>{{rental.internationalTrackingNumber}}</td>
                    <td>{{rental.customerResponse}}</td>
                    <td><button class="uk-button uk-button-text" {{action 'detail' rental}}>상세보기</button></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <hr class="uk-divider-icon">
    {{#if viewbibi}}
        <button {{action 'close'}} class="uk-close-large uk-align-right" type="button" uk-close></button>
        <form class="uk-form-stacked">
            <fieldset class="uk-fieldset">
                <legend class="uk-legend">
                    <h2 class="uk-heading-divider">주문 정보 상세보기</h2>
                </legend>
                <div class="uk-card uk-card-default uk-width-1-1@m">
                    <div class="uk-card-header">
                        <div class="uk-grid-small uk-flex-middle" uk-grid>
                            <div class="uk-width-auto">
                                <img class="uk-border-circle" width="100" height="100" src="data:image/webp;base64,">
                            </div>
                            <div class="uk-width-expand">
                                <a href="https://www.amazon.com/Power-Practical-Sparkr-Mini-Rechargeable/dp/B06XX9P4SC?SubscriptionId=AKIAJJJC67TP6CKS456A&tag=nineple0e-20&linkCode=xm2&camp=2025&creative=165953&creativeASIN=B06XX9P4SC" target="_blank">
                                    <p class="uk-card-title uk-margin-remove-bottom"><h4>Power Practical Sparkr Mini - USB Rechargeable Windproof Electric Plasma Lighter and Flashlight - Dual Arc Cigarette Lighter for Everyday Carry</h4></p>
                                </a>
                                <p class="uk-text-meta uk-margin-remove-top">By Power Practical</p>
                            </div>
                        </div>
                    </div>
                <div class="uk-card-body">
                    <div class="uk-grid-small uk-child-width-auto uk-flex-middle uk-margin-medium uk-grid">
                        <div class="uk-first-cloumn">
                            <label class="uk-form-label">오더넘버</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="0011801180001" disabled>
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">벤더넘버</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="114-6370380-4581030" disabled>
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">수량</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="2" disabled>
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">Admin</label>
                            <div class="uk-form-controls">
                                <div class="uk-inline">
                                    <button class="uk-button uk-button-default" type="button">메시지</button>
                                    <span class="uk-form-icon uk-form-icon-flip" uk-icon="icon: triangle-down"></span>
                                </div>
                                <div uk-dropdown="mode: click">
                                    <ul class="uk-nav uk-dropdown-nav">
                                        <li><a href="#">Out Of Stock</a></li>
                                        <li><a href="#">Price Change</a></li>
                                        <li><a href="#">ETC...</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="uk-grid-small uk-child-width-auto uk-flex-middle uk-margin-medium uk-grid">
                        <div class="uk-first-cloumn">
                            <label class="uk-form-label">금액</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="$29.99">
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">Sales Tax</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="$0">
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">Shipping Fee</label>
                            <div class="uk-form-controls">
                                <input class="uk-input uk-form-width-medium" type="text" placeholder="$0">
                            </div>
                        </div>
                        <div>
                            <label class="uk-form-label">변경 금액 입력</label>
                            <div class="uk-form-controls">
                                <div uk-form-custom>
                                    <input class="uk-input uk-form-width-medium" type="text" placeholder="$28.85">
                                    <button class="uk-button uk-button-default">입력</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                    <div class="uk-card-footer">
                        <div class="uk-grid-small uk-child-width-auto uk-flex-middle uk-margin-medium uk-grid">
                            <div class="uk-first-cloumn">
                                <label class="uk-form-label">ETA Message</label>
                                <div class="uk-form-controls">
                                    <div uk-form-custom>
                                        <input class="uk-input uk-form-width-large" type="text" placeholder="Estimated delivery: Jan. 22, 2018 - Jan. 26, 2018">
                                        <button class="uk-button uk-button-default">입력</button>
                                    </div>
                                </div>
                            </div>
                            <div class="uk-first-cloumn">
                                <label class="uk-form-label">고객 응답</label>
                                <div class="uk-form-controls">
                                    <input class="uk-input uk-form-width-medium" type="text" placeholder="Purchase Ok">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr class="uk-divider-icon">
                <div class="uk-align-right" uk-grid>
                    <div class="uk-width-1-1@m">  
                        <table class="uk-table">
                            <thead>
                                <tr>
                                    <th>Purchase Total</th>
                                    <th>Internaional Shipping Fee</th>
                                    <th>Total Charge</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>$29.99</td>
                                    <td>$8.25</td>
                                    <td>$38.24</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </fieldset>
        </form>
    {{/if}}
</div>

<div id="offcanvas-nav-primary" uk-offcanvas="overlay: false">
    <div class="uk-offcanvas-bar uk-flex uk-flex-column">
        <ul class="uk-nav uk-nav-primary uk-nav-center uk-margin-auto-vertical uk-nav-parent-icon" uk-nav>
            <li class="uk-active"><a href="#">주문 내역 확인</a></li>
            <li class="uk-parent">
                <a href="#">고객 관리</a>
                <ul class="uk-nav-sub">
                    <li><a href="#">고객 주문 관리</a></li>
                    <li><a href="#">고객 CS 관리</a></li>
                </ul>
            </li>
        </ul>
        <button class="uk-offcanvas-close" type="button" uk-close></button>
    </div>
</div>