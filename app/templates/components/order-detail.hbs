<div class="uk-card uk-card-default uk-width-1-1@m">
    <div class="uk-grid-small uk-flex-middle" uk-grid>
        <div class="uk-width-auto">
            <img class="uk-border-circle" width="60" height="60" src="data:image/webp;base64,">
        </div>
        {{#if viewSelect}}
            <a {{action (route-action 'modify')}} href="#" class="uk-icon-link uk-margin-small-right uk-position-right" uk-icon="icon: file-edit; ratio: 2"></a>
        {{/if}}
        <div class="uk-width-expand">
            <a href="https://www.amazon.com/Power-Practical-Sparkr-Mini-Rechargeable/dp/B06XX9P4SC?SubscriptionId=AKIAJJJC67TP6CKS456A&tag=nineple0e-20&linkCode=xm2&camp=2025&creative=165953&creativeASIN=B06XX9P4SC" target="_blank">
                <p class="uk-card-title uk-margin-remove-bottom"><h6>Power Practical Sparkr Mini - USB Rechargeable Windproof Electric Plasma Lighter and Flashlight - Dual Arc Cigarette Lighter for Everyday Carry</h6></p>
            </a>
            <p class="uk-text-meta uk-margin-remove-top">
                By Power Practical
                <img class="uk-border-circle uk-margin-right" width="50" height="50" src="https://fm.startupbridgeva.com/files/original/e10/d65/bbb/e62/398c710632620644561a.jpg">
                {{#if viewSelect}}
                    <div uk-form-custom="target: > * > span:first-child">
                        <select>
                            <option value="-1">Please select...</option>
                            <option value="1">Product OK</option>
                            <option value="2">Out of Stock</option>
                            <option value="3">Price Change</option>
                            <option value="4">Local shipping cost</option>
                        </select>
                        <button class="uk-button uk-button-default uk-width-1-1" type="button" tabindex="-1">
                            <span>주문 변경 사항</span>
                            <span uk-icon="icon: chevron-down"></span>
                        </button>
                    </div>
                {{/if}}
            </p>
        </div>
    </div>
    <table class="uk-table uk-table-small uk-text-small">
        <thead>
            <tr>
                <th>Vendor Number</th>
                <th>Local Tracking</th>
                <th>International Tracking</th>
                <th>Options</th>
                <th>Qty</th>
                <th>Price</th>
                <th>ETA Message</th>
                {{#if viewSelect}}
                    <th>Custormer Response</th>
                {{else}}
                    <th>Message from Ninapple</th>
                {{/if}}
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>114-6370380-4581030</td>
                <td>
                    <a href="https://tools.usps.com/go/TrackConfirmAction?tRef=fullpage&tLc=2&text28777=&tLabels=9374889697090034149741%2C" target="_blank">9374889697090034149741</a>
                </td>
                <td>
                    <a href="http://moniebin.com/tracking.php?no=6095003484083" target="_blank">6095003484083</a>
                </td>
                <td>color: black</td>
                <td>1</td>
                <td>29.99</td>
                <td>Arrives by Wed, Jan 31</td>
                {{#if viewSelect}}
                    <td>Purchase OK</td>
                {{else}}
                    <td>Out of Stock</td>
                {{/if}}
            </tr>
        </tbody>
    </table>
</div>
{{#if viewModify}}
  <form class="uk-form-stacked">
      <fieldset class="uk-fieldset">
          <legend class="uk-legend">
              <h2 class="uk-heading-divider">주문 내역 입력</h2>
          </legend>
          <div class="uk-grid-small uk-child-width-auto uk-flex-middle uk-margin-small uk-grid" uk-grid>
              <div class="uk-first-column">
                  <label class="uk-form-label">Vendor Number</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-medium" type="text" placeholder="114-6370380-4581030">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">Product Price</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-small" type="number" placeholder="$29.99">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">Sales Tax</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-small" type="number" placeholder="$0">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">Shipping Fee</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-small" type="number" placeholder="$0">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">ETA Message</label>
                  <div class="uk-form-controls">
                      <div uk-form-custom>
                          <input class="uk-input uk-form-width-large" type="text" placeholder="Estimated delivery: Jan. 22, 2018 - Jan. 26, 2018">
                      </div>
                  </div>
              </div>
          </div>
          <div class="uk-grid-small uk-child-width-auto uk-flex-middle uk-margin-small uk-grid" uk-grid>
              <div>
                  <label class="uk-form-label">local Tracking Carrier</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-small" type="text" placeholder="USPS">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">local Tracking Number</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-medium" type="text" placeholder="114-6370380-4581030">
                  </div>
              </div>
              <div>
                  <label class="uk-form-label">local Tracking URL</label>
                  <div class="uk-form-controls">
                      <input class="uk-input uk-form-width-medium" type="text" placeholder="https://tools.usps.com/go/TrackConfirmAction?tRef=fullpage&tLc=2&text28777=&tLabels=9374889697090034149741%2C">
                  </div>
              </div>
          </div>
          <div class="uk-margin">
              <div class="uk-text-center">
                  <button class="uk-button uk-button-primary uk-margin-small-right">Save</button>
                  <button {{action (route-action 'cancel')}} class="uk-button uk-button-danger">Cancel</button>
              </div>
          </div>
      </fieldset>
  </form>
{{/if}}
